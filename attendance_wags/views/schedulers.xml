<odoo>
	<data>
		
		<record id="daily_attendance_logs_scheduler" model="ir.cron">
            <field name="name">Daily Attendance Logs Scheduler</field>
            <field name="model_id" ref="model_daily_attendance_logs_wags"/>
            <field name="user_id" ref="base.user_admin"/>
            <field name="state">code</field>
            <field name="code">model.get_attendance()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
        
	</data>
</odoo>
